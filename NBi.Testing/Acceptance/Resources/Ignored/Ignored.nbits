<?xml version="1.0" encoding="utf-8" ?>
<testSuite name="The Query TestSuite" xmlns="http://NBi/TestSuite">
	<settings>
		<default apply-to="everywhere">
			<connectionString>Data Source=(local)\SQL2017;Initial Catalog=AdventureWorks2017;User Id=sa;password=Password12!</connectionString>
		</default>
	</settings>
	<test name="Simply Ignored" uid="0001">
    <ignore>This test is ignored</ignore>
    <category>Hello World!</category>
    <system-under-test>
      <execution>
        <query>
          select 1
        </query>
      </execution>
    </system-under-test>
    <assert>
      <all-rows>
        <predicate operand="#0">
          <more-than>1</more-than>
        </predicate>
      </all-rows>
    </assert>
  </test>
  <test name="test is no implemented" uid="0001">
    <ignore>This test is ignored</ignore>
    <category>Hello World!</category>
    <not-implemented>The predicate not-decimal is not existing</not-implemented>
    <system-under-test>
      <execution>
        <query>
          select 1
        </query>
      </execution>
    </system-under-test>
    <assert>
      <all-rows>
        <predicate operand="#0">
          <not-decimal/>
        </predicate>
      </all-rows>
    </assert>
  </test>
</testSuite>
