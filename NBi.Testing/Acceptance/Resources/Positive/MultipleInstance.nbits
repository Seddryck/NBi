<?xml version="1.0" encoding="utf-8" ?>
<testSuite name="All-rows TestSuite" xmlns="http://NBi/TestSuite">
  <settings>
    <default apply-to="system-under-test">
      <connectionString>Data Source=(local)\SQL2017;Initial Catalog=AdventureWorks2017;User Id=sa;password=Password12!</connectionString>
    </default>
  </settings>
  <variables>
    <variable name="firstDayOfCurrentMonth">
      <script language="c-sharp">new DateTime(2016, 3, 1)</script>
