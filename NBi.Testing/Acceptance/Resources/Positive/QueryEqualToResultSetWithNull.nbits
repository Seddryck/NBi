  <test name="Some keys with null and empty" uid="0030">
    <system-under-test>
      <execution>
        <query>
          select
            '001', 100
          union all
          select
            null, 300
          union all
          select
            '', 300
        </query>
      </execution>
    </system-under-test>
    <assert>
      <equal-to>
        <column index="1" role="key" type="numeric"/>
        <resultSet>
          <row>
            <cell>001</cell>
            <cell>100</cell>
          </row>
          <row>
            <cell>(null)</cell>
            <cell>300</cell>
          </row>
          <row>
            <cell>(empty)</cell>
            <cell>300</cell>
          </row>
        </resultSet>
      </equal-to>
    </assert>
  </test>
