<?xml version="1.0" encoding="utf-8" ?>
<testSuite name="Acceptance Testing: query equal-to ResultSet" xmlns="http://NBi/TestSuite">
  <settings>
    <default apply-to="everywhere">
      <connectionString>Data Source=(local)\SQL2022;Initial Catalog=AdventureWorks2022;User Id=sa;password=Password12!</connectionString>
    </default>
  </settings>
	<test name="'Reseller Order Count' by year between 2005 and 2009" uid="0001">
    <condition>
      <service-running name="MSOLAP$SQL2017TABULAR"/>
    </condition>
		<system-under-test>
			<result-set>
				<query name="MDX Except" connectionString="Provider=MSOLAP.4;Data Source=(local)\SQL2022;Initial Catalog='Adventure Works DW 2012';localeidentifier=1033">
					SELECT
					[Measures].[Reseller Order Count] ON 0,
					{[Date].[Calendar Year].[CY 2005]:[Date].[Calendar Year].[CY 2009]} ON 1
					FROM
					[Adventure Works]
				</query>
			</result-set>
		</system-under-test>
		<assert>
			<equal-to>
				<result-set>
					<row>
						<cell>CY 2005</cell>
						<cell>366</cell>
					</row>
					<row>
						<cell>CY 2006</cell>
						<cell>1015</cell>
					</row>
					<row>
						<cell>CY 2007</cell>
						<cell>1521</cell>
					</row>
					<row>
						<cell>CY 2008</cell>
						<cell>894</cell>
					</row>
					<row>
						<cell>CY 2009</cell>
						<cell/>
					</row>
				</result-set>
			</equal-to>
		</assert>
	</test>
  <test name="Some random elements with null, empty and blank compared to blank" uid="0001">
    <system-under-test>
      <result-set>
        <query>
          SELECT
          'alpha', '  '
          union all
          SELECT
          'beta', null
          union all
          SELECT
          'gamma', ''
          union all
          SELECT
          'delta', null
        </query>
      </result-set>
    </system-under-test>
    <assert>
      <equal-to values-default-type="text">
        <result-set>
          <row>
            <cell>alpha</cell>
            <cell>(blank)</cell>
          </row>
          <row>
            <cell>beta</cell>
            <cell>(blank)</cell>
          </row>
          <row>
            <cell>gamma</cell>
            <cell>(blank)</cell>
          </row>
          <row>
            <cell>delta</cell>
            <cell>(null)</cell>
          </row>
        </result-set>
      </equal-to>
    </assert>
  </test>
  <test name="Some keys with null and empty" uid="0030">
    <system-under-test>
      <result-set>
        <query>
          select
            '001', 100
          union all
          select
            null, null
          union all
          select
            '', 300
        </query>
      </result-set>
    </system-under-test>
    <assert>
      <equal-to>
        <column index="1" role="key" type="numeric"/>
        <result-set>
          <row>
            <cell>001</cell>
            <cell>100</cell>
          </row>
          <row>
            <cell>(null)</cell>
            <cell>(null)</cell>
          </row>
          <row>
            <cell>(empty)</cell>
            <cell>300</cell>
          </row>
        </result-set>
      </equal-to>
    </assert>
  </test>
</testSuite>
