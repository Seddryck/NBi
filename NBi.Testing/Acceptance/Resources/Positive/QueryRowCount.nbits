  <test name="Count of SalesTerritory is not equal to 4" uid="0001">
    <system-under-test>
      <execution>
        <query
					connectionString="Data Source=mhknbn2kdz.database.windows.net;Initial Catalog=AdventureWorks2012;User Id=sqlfamily;password=sqlf@m1ly"
				>
