  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>AdventureWorksDW</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@FirstDate">
            <Value>=Parameters!FirstDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Currencies">
            <Value>=Parameters!Currencies.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select EnglishDayNameOfWeek, CurrencyName, AverageRate from [dbo].[FactCurrencyRate]
inner join
	[dbo].[DimCurrency]
	on [dbo].[DimCurrency].[CurrencyKey] = [dbo].[FactCurrencyRate].[CurrencyKey]
inner join
	[dbo].[DimDate]
	on [dbo].[DimDate].[DateKey] = [dbo].[FactCurrencyRate].[DateKey]
where
	[FullDateAlternateKey] between @FirstDate and dateadd(day, 7, @FirstDate)
                and CurrencyName in (@Currencies)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="EnglishDayNameOfWeek">
          <DataField>EnglishDayNameOfWeek</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CurrencyName">
          <DataField>CurrencyName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="AverageRate">
          <DataField>AverageRate</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>AdventureWorksDW</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@FirstDate">
            <Value>=Parameters!FirstDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select distinct CurrencyName from [dbo].[FactCurrencyRate]
inner join
	[dbo].[DimCurrency]
	on [dbo].[DimCurrency].[CurrencyKey] = [dbo].[FactCurrencyRate].[CurrencyKey]
inner join
	[dbo].[DimDate]
	on [dbo].[DimDate].[DateKey] = [dbo].[FactCurrencyRate].[DateKey]
where
	[FullDateAlternateKey] between @FirstDate and dateadd(day, 7, @FirstDate)</CommandText>
      </Query>
      <Fields>
        <Field Name="CurrencyName">
          <DataField>CurrencyName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
