<?xml version="1.0" encoding="utf-8"?>
<testSuite name="The TestSuite" xmlns="http://NBi/TestSuite">
	<test name="Assert that a set exists">
		<system-under-test>
			<structure>
				<set caption="set" perspective="perspective" connectionString="ConnectionString"/>
			</structure>
		</system-under-test>
		<assert>
			<exists/>
		</assert>
	</test>
	<test name="Assert that a list of sets exist">
		<system-under-test>
			<structure>
        <sets perspective="perspective"/>
			</structure>
		</system-under-test>
		<assert>
      <contain>
        <item>Set1</item>
        <item>Set2</item>
      </contain>
		</assert>
	</test>
  <test name="Assert members of a set">
    <system-under-test>
      <members>
        <set caption="set" perspective="perspective" connectionString="ConnectionString"/>
      </members>
    </system-under-test>
    <assert>
      <contain>
        <item>Member1</item>
        <item>Member1</item>
      </contain>
    </assert>
  </test>
</testSuite>