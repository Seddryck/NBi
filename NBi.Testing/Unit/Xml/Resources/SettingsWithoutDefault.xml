<?xml version="1.0" encoding="utf-8" ?>
<testSuite name="The Query TestSuite" xmlns="http://NBi/TestSuite">
	<test name="My first test case" uid="0001">
		<system-under-test>
			<execution>
				<query name="Select first product">
					SELECT TOP 2 * FROM Product;
				</query>
			</execution>				
		</system-under-test>
		<assert>
			<equalTo>
				<resultSet>
					<row>
						<cell>A</cell>
						<cell>B</cell>
						<cell>1</cell>
					</row>
					<row>
						<cell>C</cell>
						<cell>D</cell>
						<cell>2</cell>
					</row>
				</resultSet>
			</equalTo>
		</assert>
	</test>
</testSuite>