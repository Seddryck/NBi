<?xml version="1.0" encoding="utf-8" ?>
<testSuite name="The Query TestSuite" xmlns="http://NBi/TestSuite">
	<settings>
		<default apply-to="assert">
			<connectionString>Provider=MSOLAP.4;Data Source=(local)\SQL2012;Initial Catalog='Adventure Works DW 2012';localeidentifier=1033</connectionString>
		</default>
	</settings>
	<test name="My first test case" uid="0001">
		<system-under-test>
			<execution>
				<query name="Select first product">
					SELECT TOP 2 * FROM Product;
				</query>
			</execution>
		</system-under-test>
		<assert>
			<equalTo>
				<query>
					SELECT TOP 3 * FROM Product;
				</query>
			</equalTo>
		</assert>
	</test>
</testSuite>