<?xml version="1.0" encoding="utf-8"?>
<testSuite name="The TestSuite" xmlns="http://NBi/TestSuite">
	<test name="level counts exactly 10 members">
		<system-under-test>
			<members>
				<level dimension="dimension" hierarchy="hierarchy" caption="level" perspective="Perspective" connectionString="ConnectionString"/>
			</members>
		</system-under-test>
		<assert>
			<count
				exactly="10"
		/>
		</assert>
	</test>
	<test name="hierarchy counts between 10 and 15 members">
		<system-under-test>
			<members>
				<hierarchy dimension="dimension" caption="hierarchy" perspective="Perspective" connectionString="ConnectionString"/>
			</members>
		</system-under-test>
		<assert>
			<count
				more-than="10"
				less-than="15"
			/>
		</assert>
	</test>
	<test name="hierarchy contains member with caption xyz under the element abc at level Level">
		<system-under-test>
			<members children-of="aBc">
				<level dimension="dimension" hierarchy="hierarchy" caption="level" perspective="Perspective" connectionString="ConnectionString"/>
			</members>
		</system-under-test>
		<assert>
			<contains
				caption="xYz"
				ignore-case="true"
			/>
		</assert>
	</test>
	<test name="level's members are ordered alphabetically">
		<system-under-test>
			<members>
				<level dimension="dimension" hierarchy="hierarchy" caption="level" perspective="Perspective" connectionString="ConnectionString"/>
			</members>
		</system-under-test>
		<assert>
			<ordered
			/>
		</assert>
	</test>
	<test name="level's members are ordered descending">
		<system-under-test>
			<members>
				<level dimension="dimension" hierarchy="hierarchy" caption="level" perspective="Perspective" connectionString="ConnectionString"/>
			</members>
		</system-under-test>
		<assert>
			<ordered
				descending="true"
			/>
		</assert>
	</test>
	<test name="level's members are ordered chronologically">
		<system-under-test>
			<members>
				<level dimension="dimension" hierarchy="hierarchy" caption="level" perspective="Perspective" connectionString="ConnectionString"/>
			</members>

		</system-under-test>
		<assert>
			<ordered
				rule="chronological"
			/>
		</assert>
	</test>
	<test name="level's members are ordered specifically">
		<system-under-test>
			<members>
				<level dimension="dimension" hierarchy="hierarchy" caption="level" perspective="Perspective" connectionString="ConnectionString"/>
			</members>
		</system-under-test>
		<assert>
			<ordered
				rule="specific"
			>
				<rule-definition>
					<item>Leopold</item>
					<item>Albert</item>
					<item>Baudoin</item>
				</rule-definition>
			</ordered>
		</assert>
	</test>
	<test name="Not available in Contains (Members)">
		<system-under-test>
			<members>
				<level dimension="dimension" hierarchy="hierarchy" caption="level" perspective="Perspective" connectionString="ConnectionString"/>
			</members>

		</system-under-test>
		<assert>
			<contains not="true" caption="member"/>
		</assert>
	</test>
	<test name="Ignore attribute" ignore="true">
		<system-under-test>
			<members>
				<level dimension="dimension" hierarchy="hierarchy" caption="level" perspective="Perspective" connectionString="ConnectionString"/>
			</members>
		</system-under-test>
		<assert>
			<contains not="true" caption="member"/>
		</assert>
	</test>
</testSuite>