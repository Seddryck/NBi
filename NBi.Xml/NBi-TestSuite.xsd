<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="NBi-TestSuite" xmlns="http://NBi/TestSuite.xsd" xmlns:mstns="http://NBi/TestSuite.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" targetNamespace="http://NBi/TestSuite.xsd">
	<xs:element name="TestSuite">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Test" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="EqualsTo" minOccurs="0">
								<xs:complexType>
									<xs:attribute name="connectionString" type="xs:string" use="required" />
									<xs:attribute name="resultSetPath" type="xs:string" use="required" />
								</xs:complexType>
							</xs:element>
							<xs:element name="SyntacticallyCorrect" minOccurs="0">
								<xs:complexType>
									<xs:attribute name="connectionString" type="xs:string" use="required" />
								</xs:complexType>
							</xs:element>
							<xs:element name="FasterThan" minOccurs="0">
								<xs:complexType>
									<xs:attribute name="connectionString" type="xs:string" use="required" />
									<xs:attribute name="maxTimeMilliSeconds" type="xs:unsignedShort" use="required" />
								</xs:complexType>
							</xs:element>
							<xs:element name="TestCase" maxOccurs="unbounded">
								<xs:complexType>
									<xs:simpleContent>
										<xs:extension base="xs:string">
											<xs:attribute name="name" type="xs:string" use="optional" />
											<xs:attribute name="file" type="xs:string" use="optional" />
										</xs:extension>
									</xs:simpleContent>
								</xs:complexType>
							</xs:element>
							<xs:element name="Category" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:simpleContent>
										<xs:extension base="xs:string">
										</xs:extension>
									</xs:simpleContent>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
						<xs:attribute name="name" type="xs:string" use="required" />
						<xs:attribute name="uid" type="xs:unsignedByte" use="optional" />
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="name" type="xs:string" use="optional" />
		</xs:complexType>
	</xs:element>
</xs:schema>